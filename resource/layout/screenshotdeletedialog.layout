"resource/layout/screenshotdeletedialog.layout" {
	styles {
		ScreenshotDeleteDialog {
			minimum-width=325	
			minimum-height=200

			render {
				2="fill(x0,y1-58,x1,y1-57, dividers_onLightBG)"
			}

			render_bg {
				0="image(x0+30,y0+64,x1,y1, graphics/icons/delete/delete)"
			}
		}

	}
	
	layout {
		place {
			control="WarningText"
			x=105
			y=58
			width=max
			margin-right=24
		}

		place {
			control="DeleteFromCloudBox"
			start="WarningText"
			dir=down
			y=4
			height=18
		}

		region {
			name="buttons"
			width=max
			height=58
			align=bottom
		}

		place {
			control="Delete,Cancel"
			region="buttons"
		    width=92
		    height=34
		    align=right
			margin-right=11
			margin-top=12
			spacing=12
		}

		place {
			control=Warning
			height=0
		}
	}
}