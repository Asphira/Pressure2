"not-transparent.styles" {
	styles {
		Notification {
			bgcolor=customgreysecondary
			textcolor=text_onDarkBG
			font-family=basefont
			font-size=16
			font-weight=400			

			render {
				0="fill(x0,y0,x0+1,y1, dividers_onDarkBG)"
				1="fill(x0+1,y0,x1,y0+1, dividers_onDarkBG)"
				2="fill(x1-1,y0+1,x1,y1, dividers_onDarkBG)"
			}
		}
	}
}